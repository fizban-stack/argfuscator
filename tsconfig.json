{
    "compileOnSave": true,
    "compilerOptions": {
        "noImplicitAny": true,
        "outFile": "./code.js",
        "preserveConstEnums": true,
        "removeComments": true,
        "sourceMap": true,
        "suppressImplicitAnyIndexErrors": true,
        "target": "ES5",
        "lib": ["dom", "es2017"]
    },
    "files": ["code.ts"],
    "include": [
        "Types/*",
        "Modifiers/*"
    ],
}
